<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp/etc/csp_whitelist.xsd">
    <policies>
        <policy id="connect-src">
            <values>
                <value id="geomapjs" type="host">www.gstatic.com</value>
                <value id="maps.googleapis" type="host">maps.googleapis.com</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="flagpedia" type="host">flagpedia.net</value>
            </values>
        </policy>
        <policy id="script-src">
            <values>
                <value id="gstatic" type="host">*.gstatic.com</value>
                <value id="maps.googleapis" type="host">maps.googleapis.com</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="gstatic" type="host">*.gstatic.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
