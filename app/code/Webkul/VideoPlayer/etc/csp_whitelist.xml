<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="connect-src">
            <values>
                <value id="cdn_plyr_io" type="host">cdn.plyr.io</value>
                <value id="noembed_com" type="host">noembed.com</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="i_ytimg_com" type="host">i.ytimg.com</value>
            </values>
        </policy>

        <policy id="frame-src">
            <values>
                <value id="youtube_access" type="host">www.youtube.com</value>
            </values>
        </policy>

        <policy id="script-src">
            <values>
                <value id="youtube_access_script" type="host">www.youtube.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
