<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="styles"/>
    <head>
        <css src="Mageplaza_GiftWrap::css/wrap-grid.css"/>
        <css src="Mageplaza_Core::css/grid-mageplaza.css"/>
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Magento\Backend\Block\Template" template="Mageplaza_GiftWrap::reports/wraps.phtml" name="mp.affiliate.reports.wraps" ifconfig="mpgiftwrap/general/show_report"/>
            <block class="Magento\Backend\Block\Template" template="Mageplaza_GiftWrap::reports/postcards.phtml" name="mp.affiliate.reports.postcards" ifconfig="mpgiftwrap/general/show_report"/>
            <uiComponent name="mpgiftwrap_wrap_grid"/>
        </referenceContainer>
    </body>
</page>
